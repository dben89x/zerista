<%= will_paginate @event_data %>
<table class = 'table'>
  <thead>
    <tr>
      <th></th>
      <% @unique_tracks.each do |track| %>
      <th class='x-axes'>
        <center>
        <%= track %>
        </center>
      </th>
      <% end %>
    </tr>
  </thead>
  <tbody>
  <% @events.each do |t|%>
  <tr>
    <td class='y-axes'>
      <%= t.start.strftime('%I:%M:%p') %> -
      <%= t.finish.strftime('%I:%M:%p') %>
    </td>

    <% count = 0 %>
    <% @unique_tracks.count.times do |a| %>
      <td>
        <% if @hash[count] == t.track %>
          <%= t.name %>
        <% end %>
      </td>
      <% count += 1 %>
    <% end %>
  </tr>
  <% end %>
  </tbody>
</table>
<%= will_paginate @event_data %>
